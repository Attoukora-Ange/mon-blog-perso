<%- include('includes/header') %>
    <div class="body-contenu contact">
        <div class="contenu_un">
            <div class="gauche">
                <div class="titre">Information générale</div>
                <div class="err" id="err"></div>
                <div class="contenu_text">
                    <form action="" method="post">
                        <div>
                            <label for="profession">Profession</label><br>
                            <input type="text" id="profession" placeholder="Ex: Pharmacien" name="profession" required>
                        </div>
                        <div class="idendite">
                            <div>
                                <div>
                                    <label for="nom">Nom</label><br>
                                    <input type="text" id="nom" placeholder="EX: Attoukora" name="nom" required>
                                </div>
                                <div>
                                    <label for="prenom">Prénom</label><br>
                                    <input type="text" id="prenom" placeholder="Ex: Ange" name="prenom" required>
                                </div>
                            </div>
                            <div>
                                <div>
                                    <label for="telephone">Téléphone / Portable</label><br>
                                    <input type="tel" id="telephone" placeholder="Ex: 0707965744" name="telephone"
                                        required>
                                </div>
                                <div>
                                    <label for="email">Email</label><br>
                                    <input type="email" id="email" placeholder="Ex: attoukoraange@gmail.com"
                                        name="email" required>
                                </div>
                            </div>
                        </div>

                        <div>
                            <div>Votre besoin</div>
                            <input type="text" id="besoin" placeholder="Ex: Posologie du paracétamol" name="besoin"
                                required>
                        </div>
                        <div>
                            <div>Votre message</div>
                            <textarea placeholder="Veuillez-nous ecrire ici ..." name="message" id="message" cols="30"
                                rows="10" required></textarea>
                        </div>
                        <div>
                            <input type="submit" value="Envoyer" id="submit">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <%- include('includes/footer') %>